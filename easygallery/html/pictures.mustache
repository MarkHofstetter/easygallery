<div class="thumbnails">
	{{#paths}}
	<div class="outer">
	{{#gps}}
	<a class="fancybox-media" href="//maps.google.com/maps?q={{name}}@{{gps.lat}},{{gps.log}}&z=14&ll={{gps.lat}},{{gps.log}}&z=15&t=m">
		<span></span>
	</a>
	{{/gps}}
	<a class="fancybox {{gpsstyle}}" href="{{path}}" rel="{{gallery}}">
		<img src="{{thumbpath}}" alt="{{name}}">
	</a>
	</div>
	{{/paths}}
</div>